{
  "database": {
    "rules": "database.rules.json"
  },
      "public": "testing",

      "headers": [ {
        "source" : "**/*.@(eot|otf|ttf|ttc|woff|index.css)",
        "headers" : [ {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        } ]
        }, {
        "source" : "**/*.@(jpg|jpeg|gif|png)",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=30672000"
        } ]
        }, {
        "source" : "404.html",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=300"
        } ]
      } ]
}
